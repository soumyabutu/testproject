/*--reset--bootstrap4 color--*/
.editor_panel_heading .panel-title {
  font-family: "GBook";
  font-size: 14px;
}
.editor_panel_heading .panel-title a {
  font-family: inherit;
  font-weight: inherit;
  color: #17a2b8;
  font-size: inherit;
  padding: 15px 15px 15px 35px;
  display: block;
  width: 100%;
  text-decoration: none !important;
  position: relative;
  border-bottom: 1px solid #ededed;
}

.editor_panel_heading .panel-title a:hover {
  color: #17a2b8;
}

.editor_panel_heading .panel-title a.collapsed,
.editor_panel_heading .panel-title a.collapsed:hover {
  color: #333;
}

.editor_panel_heading .panel-title a.collapsed:after {
  -webkit-transform: rotate(-90deg) translateY(-50%);
  -moz-transform: rotate(-90deg) translateY(-50%);
  -ms-transform: rotate(-90deg) translateY(-50%);
  transform: rotate(-90deg) translateY(-50%);
  margin-top: -7px;
  left: 25px;
}
.editor_panel_heading .panel-title a:after {
  font-family: "FontAwesome";
  content: "\f107";
  font-weight: 700;
  height: auto;
  width: auto;
  border: 0;
  color: inherit;
  vertical-align: top;
  top: 50%;
  -webkit-transform: rotate(0deg) translateY(-50%);
  -moz-transform: rotate(0deg) translateY(-50%);
  -ms-transform: rotate(0deg) translateY(-50%);
  transform: rotate(0deg) translateY(-50%);
  left: 15px;
  line-height: 100%;
  position: absolute;
  /* -webkit-transition: all .3s ease-out;
  -moz-transition: all .3s ease-out;
  -ms-transition: all .3s ease-out;
  transition: all .3s ease-out; */
}

