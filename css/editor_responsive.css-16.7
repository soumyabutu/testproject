@media (max-width: 640px) {
  .right_page_section {
    margin-left: 0px;
    width: 100%;
  }
  .float_right_panel {
    height: 65px;
    overflow: visible;
    width: calc( 100% - 160px );
    position: absolute;
    right: 0;
  }
  .left_setting_section {
    width: 100%;
    border-right: 0px;
    border-bottom: 1px solid #dadada;
  }
  .fixed_lf_panel {
    height: calc( 100vh - 66px );
    position: static;
  }
  .suffle_page_contents {
    display: none;
  }
  .theme_device_view {
    display: none;
  }
  .mid_page_list_setting_panel {
    height: calc(100vh - 130px);
    margin-top: 65px;
  }

  .theme_page_dropdown {
    display: none;
  }

  .ft_sec {
    position: fixed;
    bottom: 0;
    left: 0;
    right: 0;
    width: 160px;
    box-shadow: none;
    background-color: #fff;
  }

  .float_right_panel .rt_header_panel {
    box-shadow: none;
    background-color: #fff;
  }
  .ft_sec {
    box-shadow: none;
  }

  .fixed_header {
    position: absolute;
    top: 0px;
    left: 0px;
    right: 0px;
    width: 100%;
    background-color: #fff;
  }

  .page_content_editor_panel_inner   .fixed_header {
    position: static;
  }

  .list_setting_tab-content {
    padding: 15px 10px 15px;
  }
  .list_setting_panel .flex_nav-link {
    padding: 10px 0px 6px;
  }

  .list_setting_tab-content {
    height: calc(100% - 35px);
  }

  .all_editor_content {
    height: 100%;
    padding-bottom: 15px;
    height: calc(100% - 66px);
  }
  .add_editor_panel_inner .back_to_btn,
  .inner_page_content_editor_panel .back_to_btn {
    width: 22px;
    margin-left: auto !important;
  }
  .add_editor_panel_inner {
    padding-bottom: 15px;
  }

  .add_editor_panel_inner .all_editor_content {
    height: calc(100% - 52px);
  }
  .page_content_editor_panel_inner .all_editor_content,
  .inner_page_content_editor_panel .all_editor_content{
      height: calc(100vh - 132px);
  }

}
